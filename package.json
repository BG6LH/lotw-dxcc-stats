{
  "name": "lotw-dxcc-stats",
  "version": "0.1.0",
  "type": "module",
  "main": "index.js",
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.cjs"
    },
    "./lib/*": "./lib/*",
    "./schemas/*": "./schemas/*"
  },
  "bin": {
    "update-stats": "./bin/update-stats"
  },
  "files": [
    "lib/",
    "bin/",
    "schemas/",
    "index.js",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "axios": "^1.9.0",
    "commander": "^11.1.0",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "jest": "^29.7.0"
  },
  "peerDependencies": {
    "@11ty/eleventy": ">=2.0.0"
  },
  "peerDependenciesMeta": {
    "@11ty/eleventy": {
      "optional": true
    }
  },
  "description": "A tool for fetching and processing DXCC data from Logbook of the World (LoTW)",
  "scripts": {
    "update-stats": "node bin/update-stats",
    "validate-data": "ajv validate -s schemas/lotw-dxcc-data.schema.json -d output/lotwDxcc.json",
    "test": "jest",
    "lint": "eslint lib/ bin/ index.js",
    "format": "prettier --write .",
    "prepublishOnly": "npm run lint && npm run test",
    "prepare": "npm run format"
  },
  "keywords": [
    "lotw",
    "dxcc",
    "ham-radio",
    "amateur-radio",
    "qsl",
    "qso",
    "adif",
    "eleventy",
    "data-fetcher",
    "arrl",
    "logbook"
  ],
  "author": {
    "name": "BG6LH",
    "email": "bg6lh@qsl.net",
    "url": "https://github.com/BG6LH"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BG6LH/lotw-dxcc-stats.git"
  },
  "bugs": {
    "url": "https://github.com/BG6LH/lotw-dxcc-stats/issues"
  },
  "homepage": "https://github.com/BG6LH/lotw-dxcc-stats#readme"
}
